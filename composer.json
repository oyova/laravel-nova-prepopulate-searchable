{
    "name": "oyova/laravel-nova-prepopulate-searchable",
    "description": "A tool to allow BelongsTo searchable fields to be pre-populated with data.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/oyova/laravel-nova-prepopulate-searchable",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Bronner",
            "email": "hello@genealabs.com",
            "role": "Developer"
        },
        {
            "name": "Alex Bowers",
            "email": "bowersbros@gmail.com",
            "role": "Original Creator"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/nova": "^3.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "oyova/php-cs-fixer": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Oyova\\LaravelNovaPrepopulateSearchable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oyova\\LaravelNovaPrepopulateSearchable\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
